/*
 * Playbook contains multiple rules which checks lacp statistics
 * and notifies when anomalies are found.
 *
 * 1) Rule "check-lacp-state-netconf" checks statistics on lacp enabled interfaces
 *    and notifies anomalies.
 */
healthbot {
    playbook lacp-kpis-openconfig {
        rules [ protocol.lacp/check-lacp-state-netconf ];
        description "checks links are sending/receiving lacp packets ";
        synopsis "lacp key performance indicators";
    }
}
